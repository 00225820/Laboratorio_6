#include <iostream>
using namespace std;


int contador(int array[], int num1, int tamanio, int num, int resultado)
{
    int resultado = 0;

    for (int num = 0; num < tamanio; resultado++)
    {
      if(array[num] == num1){
        resultado ++;
        cout << "El numero de repeticiones del numero colocado en el array es:" << resultado;
      }
    }

    return num1;
}


